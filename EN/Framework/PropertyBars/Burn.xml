<?xml version="1.0" encoding="iso-8859-1"?>
<painter-ui xmlns="Corel::Painter::Framework"
						xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
						xsi:schemaLocation="http://pntrwin-d-xp/schema/12.0/
																http://pntrwin-d-xp/schema/12.0/PainterFramework.xsd">
																
	<container flow="equal-horizontal" theme="property-bar-theme" spacing="0">

		<container flow="equal-vertical" height="value:54;minimum:34;maximum:54;" width="value:89;minimum:89;maximum:89;" spacing="0">
			<label caption="ToolsPropertyBar|ResetLabel" text-style="h-center" height="value:20;minimum:20;maximum:20;" font-size="12" visible-init="data-source:property-bar-with-labels;" />
			<container flow="fill" spacing="0">
		
				<image-button 
							src="Property Bars/Photo/Reset Burn.png" 
							tool-tip="PropertyBar|ResetTool"
							onClick="proxy: function='Reset Burn';"/>

			</container>

		</container>

		<divider orientation="vertical" height="value:52;minimum:32;maximum:52;"/>
		
		<extern src="PropertyBars/BrushExtern.xml" />

		<divider orientation="vertical" height="value:52;minimum:32;maximum:52;"/>

		<container flow="equal-horizontal" theme="property-bar-theme" height="value:54;minimum:34;maximum:54;" spacing="0" auto-size="true">
				<container flow="equal-vertical" height="value:54;minimum:34;maximum:54;" width="value:230;minimum:230;maximum:230;" spacing="0">
					<label caption="PropertyBar|MediaLabel" text-style="h-center" height="value:20;minimum:20;maximum:20;" font-size="12" visible-init="data-source:property-bar-with-labels;" />

					<container flow="equal-horizontal" spacing="x:2">
						<divider stroke-width="0" width="value:5;minimum:5;maximum:5;" os="win" />
						<divider stroke-width="0" width="value:2;minimum:2;maximum:2;" os="mac" />

						<container flow="equal-horizontal">
							<extern src="PropertyBars/Brushes/resat_bleed_extern.xml" />
						</container>

					</container>
				</container>
				<divider orientation="vertical" height="value:52;minimum:32;maximum:52;"/>
		</container>
	
	</container>

</painter-ui>
