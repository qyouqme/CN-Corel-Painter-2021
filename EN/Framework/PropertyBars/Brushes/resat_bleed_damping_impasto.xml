<?xml version="1.0" encoding="utf-8"?>
<painter-ui xmlns="Corel::Painter::Framework"
						xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
						xsi:schemaLocation="http://pntrwin-d-xp/schema/12.0/
																http://pntrwin-d-xp/schema/12.0/PainterFramework.xsd">
	<container flow="equal-horizontal" theme="property-bar-theme" height="value:54;minimum:34;maximum:54;" spacing="0" auto-size="true">
		<container flow="equal-vertical" height="value:54;minimum:34;maximum:54;" width="value:366;minimum:366;maximum:366;" spacing="0">
			<label caption="PropertyBar|MediaLabel" text-style="h-center" height="value:20;minimum:20;maximum:20;" font-size="12" visible="data-source:property-bar-with-labels;"/>
			<container flow="equal-horizontal" spacing="0">
				<divider stroke-width="0" width="value:5;minimum:5;maximum:5;" os="win"/>
				<divider stroke-width="0" width="value:4;minimum:4;maximum:4;" os="mac"/>

				<container flow="equal-horizontal" auto-size="true" spacing="x:-3">
					<template name="label-edit-dropdown">
						<var name="caption" value="Damping|5_caption"/>
						<var name="tooltip" value="Damping|5_tooltip"/>
						<var name="min" value="0.00999451"/>
						<var name="max" value="1"/>
						<var name="step" value="0.00999451"/>
						<var name="format" value="percent"/>
						<var name="precision" value="0"/>
						<var name="data_source" value="proxy&lt;Fixed&gt;: update-category='Damping'; function='Set Damping';"/>
						<var name="enabled" value="data-source: proxy: update-category='Damping';"/>
					</template>
				</container>

				<divider orientation="horizontal" stroke-width="0" width="value:5;minimum:5;maximum:5;" os="mac" />
				<divider orientation="horizontal" stroke-width="0" width="value:8;minimum:8;maximum:8;" os="win" />

				<container flow="equal-horizontal">
					<extern src="PropertyBars/Brushes/resat_bleed_extern.xml"/>
				</container>

				<container flow="equal-horizontal" width="value:24;minimum:24;maximum:24;">
					<extern src="PropertyBars/Brushes/blending_slotted_extern.xml"/>
				</container>

				<divider stroke-width="0" width="value:6;minimum:6;maximum:6;" />

				<container flow="equal-horizontal" width="value:24;minimum:24;maximum:24;">
					<extern src="PropertyBars/Brushes/impasto_slotted_extern.xml" />
				</container>
			</container>
		</container>

		<divider stroke-width="0" width="value:7;minimum:7;maximum:7;" os="win"/>
		<divider stroke-width="0" width="value:8;minimum:8;maximum:8;" os="mac"/>
		<divider orientation="vertical" height="value:52;minimum:32;maximum:52;"/>
	</container>
</painter-ui>
